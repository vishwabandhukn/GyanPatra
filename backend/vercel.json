{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60,
                "memory": 1024
            }
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "*/15 * * * *"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}